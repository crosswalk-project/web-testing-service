<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="W3C/HTML5 APIs" name="webapi-imports-w3c-tests">
    <set name="Imports" type="js">
      <testcase component="WebAPI/Web Components/HTML Imports" execution_type="auto" id="LinkImport_import_not_import" purpose="Check if the import attribute must return null when the link does not represent an import">
        <description>
          <test_script_entry>/opt/webapi-imports-w3c-tests/imports/LinkImport_import_not_import.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Web Components/HTML Imports" execution_type="auto" id="LinkImport_import_not_in_document" purpose="Check if the import attribute must return null when the link element is not in a Document">
        <description>
          <test_script_entry>/opt/webapi-imports-w3c-tests/imports/LinkImport_import_not_in_document.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Web Components/HTML Imports" execution_type="auto" id="LinkImport_import_set_removed_flag" purpose="Check if the import attribute must return null when the element removed flag of the import is set">
        <description>
          <test_script_entry>/opt/webapi-imports-w3c-tests/imports/LinkImport_import_set_removed_flag.html</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Web Components/HTML Imports" execution_type="auto" id="LinkImport_import_valid" purpose="Check if the import attribute must return the imported document for the import">
        <description>
          <test_script_entry>/opt/webapi-imports-w3c-tests/imports/LinkImport_import_valid.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
