<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="./testcase.xsl"?>
<test_definition>
  <suite category="W3C/HTML5 APIs" name="webapi-hrtime-w3c-tests">
    <set name="hrtime" type="js">
      <testcase component="WebAPI/Performance and Optimization/High Resolution Time" execution_type="auto" id="Performance_monotonic-clock_001" purpose="Check if the window.performance.now() returns a positive number">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/monotonic-clock.html?total_num=2&amp;amp;locator_key=id&amp;amp;value=1</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Performance and Optimization/High Resolution Time" execution_type="auto" id="Performance_monotonic-clock_002" purpose="Check if the window.performance.now() difference is not negative">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/monotonic-clock.html?total_num=2&amp;amp;locator_key=id&amp;amp;value=2</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Performance and Optimization/High Resolution Time" execution_type="auto" id="Performance_test_cross_frame_start_001" purpose="Check if the parent performance.now() is greater than 0">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/test_cross_frame_start.html?total_num=4&amp;amp;locator_key=id&amp;amp;value=1</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Performance and Optimization/High Resolution Time" execution_type="auto" id="Performance_test_cross_frame_start_002" purpose="Check if the child performance.now() is greater than 0">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/test_cross_frame_start.html?total_num=4&amp;amp;locator_key=id&amp;amp;value=2</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Performance and Optimization/High Resolution Time" execution_type="auto" id="Performance_test_cross_frame_start_003" purpose="Check if the child performance.now() created at least 1 second after parent performance.now()">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/test_cross_frame_start.html?total_num=4&amp;amp;locator_key=id&amp;amp;value=3</test_script_entry>
        </description>
      </testcase>
      <testcase component="WebAPI/Performance and Optimization/High Resolution Time" execution_type="auto" id="Performance_test_cross_frame_start_004" purpose="Check if the values of the window.performance.now() are based on the current document's navigationStart">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/test_cross_frame_start.html?total_num=4&amp;amp;locator_key=id&amp;amp;value=4</test_script_entry>
        </description>
      </testcase>
    </set>
    <set name="idltest" type="js">
      <testcase component="WebAPI/Performance and Optimization/High Resolution Time" execution_type="auto" id="Performance_idlharness" purpose="Check if the Performance idlharness tests">
        <description>
          <test_script_entry>/opt/webapi-hrtime-w3c-tests/hrtime/w3c/idlharness.html</test_script_entry>
        </description>
      </testcase>
    </set>
  </suite>
</test_definition>
